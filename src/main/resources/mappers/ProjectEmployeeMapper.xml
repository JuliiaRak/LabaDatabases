<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="solvd.laba.itcompany.persistence.ProjectEmployeeRepository">

    <insert id="create" parameterType="solvd.laba.itcompany.domain.ProjectEmployee">
        INSERT INTO Project_Employees (project_id, employee_id, role)
        VALUES (#{project.id}, #{employee.id}, #{role});
    </insert>

</mapper>
